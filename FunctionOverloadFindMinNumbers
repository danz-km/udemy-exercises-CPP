#include<iostream>
using namespace std;


//1 write a Min() function here to Min of 2 integers
int Min(int x, int y){
    if (x>y)
        return y;
    else
        return x;
}
//2 write a Min() function here to Min of 3 integers
int Min(int a, int b, int c){
    //a = 5; b = 3, c = 2
    if (a < b && a < c)
    //true, true, true
        return a;
    else if (b < a && b < c)
        return b;
    else 
        return c;
}

//3 write a Min() function here to Min of 2 floats
float Min(float g, float h){
    if (g>h)
        return h;
    else
        return g;
}

void Main()
{
    cout<<Min(10,5);
    cout<<Min(12,7,9);
    cout<<Min(18.0f,9.0f);
}
